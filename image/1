	<!--  Page required Script -->
	<script type="d934386925940e28863d376a-text/javascript">
    // Set the percentage (change this value as needed)
    var percentage = $('#progressCount').val();
    var rotationClass = '';
    if (percentage >= 10 && percentage <= 35) {
        rotationClass = 'pink';
    } else if (percentage > 35 && percentage <= 50) {
        rotationClass = 'yellow';
    } else if (percentage > 50 && percentage <= 75) {
        rotationClass = 'green';
    } else if (percentage > 75) {
        rotationClass = 'blue';
    }
</script>

	<!--  End Page required Script -->

