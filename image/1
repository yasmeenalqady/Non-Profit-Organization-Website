     let navbarTogglerIcon = document.querySelector('.navbar-toggler');

     function hideHamburgerIcon() {
          navbarTogglerIcon.classList.add('d-none');
     }

     function showHamburgerIcon() {
          navbarTogglerIcon.classList.remove('d-none');
     }
